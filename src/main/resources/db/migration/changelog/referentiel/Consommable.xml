<!-- db/changelog-1.1-container.xml -->

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

  <!-- Create Container Table -->
    <changeSet id="1" author="your-name">
    <createTable tableName="consommable">
      <column name="id" type="BIGINT">
        <constraints primaryKey="true" nullable="false"/>
      </column>
        <column name="code" type="VARCHAR(500)"/>
        <column name="libelle" type="VARCHAR(500)"/>
        <column name="description" type="VARCHAR(500)"/>
        <column name="style" type="VARCHAR(500)"/>

        <column name="unite_id" type="BIGINT">
          <constraints nullable="false"/>
        </column>

    </createTable>
  </changeSet>

    <!-- Add foreign key constraint -->
  <changeSet id="2" author="your-name">
      <addForeignKeyConstraint
              baseTableName="consommable"
              baseColumnNames="unite_id"
              referencedTableName="unite"
              referencedColumnNames="id"
              constraintName="fk_consommable_unite"/>

  </changeSet>

    <!-- Create Sequence for ContainerType -->
  <changeSet id="3" author="your-name">
    <createSequence sequenceName="consommable_seq" startValue="1" incrementBy="1"/>
  </changeSet>

  <!-- Load Data from CSV -->
  <changeSet id="4" author="your-name">
    <loadData
            file="classpath:/db/migration/data/referentiel/Consommable.csv"
            tableName="consommable">
      <column name="id" type="NUMERIC"/>
          <column name="code" type="STRING"/>
          <column name="libelle" type="STRING"/>
          <column name="description" type="STRING"/>
          <column name="style" type="STRING"/>
        <column name="unite" type="BIGINT"/>
    </loadData>
  </changeSet>


</databaseChangeLog>
